{
  "extends": "./.nitro/types/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "jsxFragmentFactory": "React.Fragment",
    "allowImportingTsExtensions": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "paths": {
      "~/.vitepress/*": [
        "./.vitepress/*"
      ],
    },
  },
}
